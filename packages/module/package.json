{
  "name": "@cloudmosaic/quickstarts",
  "version": "0.0.25",
  "description": "Quickstarts for MAS",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudmosaic/quickstarts.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloudmosaic/quickstarts/issues"
  },
  "homepage": "https://github.com/cloudmosaic/quickstarts#readme",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "build": "rollup -c && yarn post-css",
    "watch": "rollup -cw",
    "test": "jest",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "yarn eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "yarn type-check && yarn lint && yarn test",
    "build:ts": "yarn clean && tsc --p tsconfig.json",
    "post-css": "yarn clean-css",
    "purge-css": "purgecss --css dist/quickstarts.css --content dist/**/*.js --output dist",
    "clean-css": "cleancss dist/quickstarts.css -o dist/quickstarts.min.css"
  },
  "peerDependencies": {
    "@patternfly/patternfly": ">=4.90.5",
    "@patternfly/react-catalog-view-extension": ">=4.10.13",
    "@patternfly/react-core": ">=4.101.3",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
